{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@api": ["api/api"],
      "@atom/*": ["atom/*"],
      "@container/*": ["common/container/*"],
      "@errorContainer/*": ["common/errorContainer/*"],
      "@footer/*": ["common/footer/*"],
      "@header/*": ["common/header/*"],
      "@installContainer/*": ["common/installContainer/*"],
      "@modal/*": ["common/modal/*"],
      "@sidebar/*": ["common/sidebar/*"],
      "@userContainer/*": ["common/userContainer/*"],
      "@utils/*": ["common/utils/*"],
      "@accountComp/*": ["components/account/*"],
      "@guideComp/*": ["components/guide/*"],
      "@infoComp/*": ["components/info/*"],
      "@loginComp/*": ["components/login/*"],
      "@mainComp/*": ["components/main/*"],
      "@messagesComp/*": ["components/messages/*"],
      "@mypage/*": ["components/my-page/*"],
      "@registerComp/*": ["components/register/*"],
      "@searchComp/*": ["components/search/*"],
      "@searchOptionComp/*": ["components/search/option/*"],
      "@searchPharmComp/*": ["components/search/pharmacy/*"],
      "@searchResultComp/*": ["components/search/result/*"],
      "@searchResultListComp/*": ["components/search/resultList/*"],
      "@hooks/*": ["hooks/*"],
      "@lib/*": ["lib/*"],
      "@modelTypes/*": ["src/model/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "firebase/index.js"],
  "exclude": ["node_modules"]
}
